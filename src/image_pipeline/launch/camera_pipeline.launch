<launch>
  <group ns="camera_down">
    <node pkg="libuvc_camera" type="camera_node" name="camera1_node">
      <!-- Parameters used to find the camera -->
      <param name="frame_id" value="camera1_frame" />
      <param name="vendor" value="0x0bda"/>
      <param name="product" value="0x5842"/>
      <param name="width" value="640"/>
      <param name="height" value="480"/>
      <param name="video_mode" value="mjpeg"/>
      <param name="frame_rate" value="30"/>
    </node>
  </group>
 <group ns="camera_front">
    <node pkg="perception" type="publish_cameras" name="publish_cameras">
    </node>
 </group>
</launch>
